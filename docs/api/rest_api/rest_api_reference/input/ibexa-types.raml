            description: Asset's URI address.
        assetId:
            type: string
            description: Asset's identifier.
        assetSource:
            type: string
            description: Asset provider's identifier.
        assetMetadata:
            type: assetMetadata
            description: 'Asset metadata object. Depending on the provider, it can contain different parameters.'

assetMetadata:
    type: object
    description: 'Asset metadata object. Depending on the provider, it can contain different parameters.'
    properties:
        alternativeText:
            type: string
