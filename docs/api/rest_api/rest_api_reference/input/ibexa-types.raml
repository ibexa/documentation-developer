            type: datetime
            description: Image asset's creation date
        updated_at:
            type: datetime
            description: Image asset's modification date
        author:
            type: string
            description: 'A name of the author of the image asset'
        author_link_html:
            type: string
            description: 'A URL address of the author of the image asset'

BaseObject:
    type: object
    properties:
        _media-type:
            type: string
            xml:
                attribute: true
                name: media-type
        _href:
            type: string
