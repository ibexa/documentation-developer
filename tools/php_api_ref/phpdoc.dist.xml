<?xml version="1.0" encoding="UTF-8" ?>
<!-- https://docs.phpdoc.org/guide/references/configuration.html -->
<phpdocumentor
        configVersion="3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="https://www.phpdoc.org"
>
    <title>PHP API Reference</title>
    <version number="*">
        <api>
            <default-package-name>Ibexa</default-package-name>
            <source dsn=".">
                <path>vendor/ibexa/</path>
                <path>vendor/ezsystems/</path>
                <path>vendor/silversolutions/</path>
            </source>
            <ignore hidden="true" symlinks="true">
                <path>vendor/**/tests/**/*</path>
                <path>vendor/**/spec/**/*</path>

                <!-- SebastianBergmann\Diff -->
                <path>vendor/ezsystems/diff/**/*</path>

                <!-- App\QueryType\NearbyPlacesQueryType -->
                <path>vendor/ibexa/fieldtype-query/doc/examples/nearby_places/NearbyPlacesQueryType.php</path>

                <!-- Hautelook\TemplatedUriBundle -->
                <path>vendor/ibexa/templated-uri-bundle/Hautelook/**/*</path>

                <!-- Hautelook\TemplatedUriRouter -->
                <path>vendor/ibexa/templated-uri-router/**/*</path>
            </ignore>
            <!-- Hide private elements -->
            <visibility>public</visibility>
            <visibility>protected</visibility>
        </api>
    </version>
</phpdocumentor>
