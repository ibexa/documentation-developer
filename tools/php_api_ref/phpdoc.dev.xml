<?xml version="1.0" encoding="UTF-8" ?>
<!-- https://docs.phpdoc.org/guide/references/configuration.html -->
<phpdocumentor
        configVersion="3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="https://www.phpdoc.org"
>
    <title>PHP API Reference</title>
    <version number="*">
        <api>
            <default-package-name>Ibexa</default-package-name>
            <source dsn=".">
                <path>vendor/ibexa/admin-ui/src/contracts/**/*</path>
                <path>vendor/ibexa/calendar/src/contracts/**/*</path>
                <path>vendor/ibexa/checkout/src/contracts/**/*</path>
            </source>
            <ignore hidden="true" symlinks="true">
                <path>vendor/**/tests/**/*</path>
                <path>vendor/**/spec/**/*</path>
            </ignore>
            <!-- Hide private elements -->
            <visibility>public</visibility>
            <visibility>protected</visibility>
        </api>
    </version>
</phpdocumentor>
